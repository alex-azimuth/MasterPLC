{"CLs":[{"__type":"IST","EID":"f3e6b85d-b897-4064-8abd-fb50245569f9","H":1294.8,"TXT":"\/\/ Manual feed messages\u000d\u000aIF (HMI_Feeder_Data.Manual_Feeding) THEN\u000d\u000a\u0009HMI_Feeder_Data.HMI_Transfer_Msg := 'Manual Mode';\u000d\u000a\u0009IF (Entry_Feeder_Jog.Busy) THEN\u000d\u000a\u0009\u0009HMI_Feeder_Data.HMI_Transfer_Msg := 'Jogging...';\u000d\u000a\u0009ELSIF (Main_Feeder_Micro.Busy) THEN\u000d\u000a\u0009\u0009HMI_Feeder_Data.HMI_Transfer_Msg := 'Micro Feeding...';\u000d\u000a\u0009END_IF;\u000d\u000a\u0009IF (Entry_Feeder_Jog.Busy) THEN\u000d\u000a\u0009\u0009HMI_Feeder_Data.HMI_Transfer_Msg := 'Entry Jogging...';\u000d\u000a\u0009ELSIF (Entry_Feeder_Micro.Busy) THEN\u000d\u000a\u0009\u0009HMI_Feeder_Data.HMI_Transfer_Msg := 'Entry Micro Feeding...';\u000d\u000a\u0009END_IF;\u000d\u000a\u0009IF (Exit_Feeder_Jog.Busy) THEN\u000d\u000a\u0009\u0009HMI_Feeder_Data.HMI_Transfer_Msg := 'Exit Jogging...';\u000d\u000a\u0009ELSIF (Exit_Feeder_Micro.Busy) THEN\u000d\u000a\u0009\u0009HMI_Feeder_Data.HMI_Transfer_Msg := 'Exit Micro Feeding...';\u000d\u000a\u0009END_IF;\u000d\u000a\/\/ Auto feed messages, press+feeder combo\u000d\u000aELSIF (HMI_Feeder_Data.Auto_Feeding) AND (NOT Master_Feeder_Options.Wintriss_Feeder) THEN\u000d\u000a\u0009HMI_Feeder_Data.HMI_Transfer_Msg := 'Auto waiting...';\u000d\u000a\u0009IF (Main_Feed_Move.Busy) THEN\u000d\u000a\u0009\u0009HMI_Feeder_Data.HMI_Transfer_Msg := 'Feeding...';\u000d\u000a\u0009ELSIF (Entry_Feed_Move.Busy) THEN\u000d\u000a\u0009\u0009HMI_Feeder_Data.HMI_Transfer_Msg := 'Entry Feeding...';\u000d\u000a\u0009ELSIF (Exit_Feed_Move.Busy) THEN\u000d\u000a\u0009\u0009HMI_Feeder_Data.HMI_Transfer_Msg := 'Exit Feeding...';\u000d\u000a\u0009END_IF;\u000d\u000a\u0009IF (Pause_Feed) THEN\u000d\u000a\u0009\u0009HMI_Feeder_Data.HMI_Transfer_Msg := 'Feed paused...';\u000d\u000a\u0009END_IF;\u000d\u000a\u0009IF (Press_Signals.Main_Feeder_Release_Zone) THEN\u000d\u000a\u0009\u0009HMI_Feeder_Data.HMI_Transfer_Msg := 'Releasing..